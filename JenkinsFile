pipeline {
    agent any 

    environment{
        PATH = "C:\\WINDOWS\\SYSTEM32"
    }

    stages {
        stage('fetch'){
            steps {
                bat 'echo "fetching"'
            }
        }  
        stage('build'){
            steps {
                bat 'echo "building"'
            }
            }
        stage('test'){
            steps {
                bat 'echo "testing"'
            }
        }
        stage('deploy'){
            steps {
                bat 'echo "deploying"'
            }
        }  
    }
}